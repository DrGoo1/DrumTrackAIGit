# DrumTracKAI Environment Variables
# Copy this file to .env and fill in your actual values

# MVSep API Key (required for stem separation)
MVSEP_API_KEY=your_mvsep_api_key_here

# Optional: Enable debug mode for MVSep
MVSEP_DEBUG=false

# Optional: Skip connectivity test for faster startup
MVSEP_SKIP_CONNECTIVITY_TEST=false

# LLVM Environment Variables (for audio analysis)
NUMBA_DISABLE_INTEL_SVML=1
OMP_NUM_THREADS=1
